    "def factorialRecursivo(n):\n",
    "    f = 0\n",
    "    if n == 0 or n == 1:\n",
    "        f = 1\n",
    "        return f\n",
    "    else:\n",
    "        f = n * factorialRecursivo(n -1)\n",
    "        return f\n",
    "print(factorialRecursivo(4))"
   ]
